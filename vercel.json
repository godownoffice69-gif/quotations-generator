{
  "version": 2,
  "redirects": [
    {
      "source": "/",
      "destination": "/admin/",
      "permanent": false,
      "has": [
        {
          "type": "host",
          "value": "fp-order-management-system.vercel.app"
        }
      ]
    },
    {
      "source": "/admin",
      "destination": "/admin/",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/admin/service-worker.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/admin/"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}
